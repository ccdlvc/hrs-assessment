{
  "name": "elasticsearch-hotel-sink",
  "config": {
    "connector.class": "io.confluent.connect.elasticsearch.ElasticsearchSinkConnector",
    "type.name": "elasticsearch-hotel-sink",
    "behavior.on.null.values": "delete",
    "errors.log.include.messages": "true",
    "tasks.max": "1",
    "group.id": "connect-elasticsearch-hotel-sink",
    "max.retries": "100",
    "key.ignore": "false",
    "retry.backoff.ms": "100",
    "max.buffered.records": "3000",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "errors.log.enable": "true",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "read.timeout.ms": "10000",
    "batch.size": "100",
    "max.in.flight.requests": "5",
    "schema.ignore": "true",
    "key.converter.schemas.enable": "false",
    "value.converter.schemas.enable": "false",
    "name": "elasticsearch-hotel-sink",
    "errors.tolerance": "all",
    "connection.url": "http://elasticsearch:9200",
    "connection.username": "elastic",
    "connection.password": "hrs.com",
    "schemas.enable": "false",
    "linger.ms": "10000",
    "topics": "connect.hrs_booking.hotels",
    "transforms": "cdcToEsHotel",
    "transforms.cdcToEsHotel.type": "com.hrs.transforms.CDCToESHotelTransformation"
  }
}
